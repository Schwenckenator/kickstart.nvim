{
	"reactCustomHook": {
		"prefix": "rh",
		"body": [
			"export default function ${TM_FILENAME_BASE} {",
			"  $0",
			"}\n"
		],
		"description": "Creates a custom hook"
	},
	"reactUseEffect": {
		"prefix": "ue",
		"body": [
			"useEffect(() => {",
			"  $0",
			"}, [$1])\n"
		],
		"description": "React useEffect hook"
	}
}
